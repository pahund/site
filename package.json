{
  "name": "react-finland-site",
  "version": "0.0.0",
  "description": "React Finland site",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node ./antwar.bootstrap.js develop",
    "build":
      "rimraf ./build && node ./antwar.bootstrap.js build && npm run sitemap && npm run copy:all",
    "build:netlify": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint . --ignore-path .gitignore --fix",
    "test": "npm run lint",
    "sitemap":
      "cd build && sitemap-static --prefix=https://react-finland.fi/ > sitemap.xml",
    "copy:all": "npm run copy:extra",
    "copy:extra": "cp -rfv assets/extra/* build",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ReactFinland/site.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ReactFinland/site/issues"
  },
  "homepage": "https://github.com/ReactFinland/site#readme",
  "dependencies": {
    "@react-finland/content-2018": "^12.3.2",
    "antwar": "^0.27.1",
    "antwar-interactive": "^0.21.0",
    "apollo-client": "^2.2.6",
    "classnames": "^2.2.5",
    "graphql": "^0.12.3",
    "graphql-request": "^1.5.1",
    "graphql-tools": "^2.21.0",
    "moment": "^2.21.0",
    "normalize.css": "^8.0.0",
    "preact-render-to-string": "^3.7.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-country-flag": "^0.2.0",
    "react-dom": "^16.2.0",
    "react-helmet-async": "0.0.5",
    "react-image-lightbox": "^4.5.0",
    "react-markdown": "^3.3.0",
    "react-photo-gallery": "^6.0.22",
    "react-router-dom": "^4.2.2",
    "timediff": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "autoprefixer": "^8.1.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0-beta.0",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.1",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "prettier": "^1.11.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^4.0.0",
    "react-router": "^4.2.0",
    "react-styleguidist": "^6.2.7",
    "redirect-webpack-plugin": "^0.1.1",
    "request": "^2.83.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "sitemap-static": "^0.4.2",
    "style-loader": "^0.20.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.1",
    "webpack-dev-server": "^3.1.0",
    "webpack-merge": "^4.1.2"
  },
  "lint-staged": {
    "*.{js,jsx,css,scss,json}": ["prettier --write", "git add"]
  }
}
